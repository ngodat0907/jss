<body>
    <script>
        let numbercoca = 5;
        let cocaprice = 10000;
        let cocamoney = cocaprice * numbercoca;
        let VAT = cocamoney * (5 / 100);

        let numberwine = 5;
        let winecapacity = 5;
        let wineprice = 15000;
        let winemoney = numberwine * winecapacity * wineprice;

        console.log("tổng tiền gồm thuế = " + (VAT + cocamoney + winemoney));
        console.log("tổng tiền chưa thuế = " + (cocamoney + winemoney));
        console.log("tổng tiền 5 lon nước = " + cocamoney);
        console.log("tông tiền 5 chai rượu = " + winemoney);
        
    </script>
</body>
